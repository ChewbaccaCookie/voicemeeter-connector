{
    "compilerOptions": {
        "allowJs": true,
        "declaration": true,
        "esModuleInterop": true,
        "lib": ["esnext"],
        "module": "es2022",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "dist",
        "skipLibCheck": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es2022"
    }
}
